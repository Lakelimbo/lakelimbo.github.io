---
const { h1, p1, p2, langSwitch, langSwitchLink = "#", contact } = Astro.props;
---

<main id="hero">
   <h1>{h1}</h1>
   <p>
      {p1}
   </p>
   <p>
      {p2}
   </p>
   <a href={langSwitchLink} id="lang-switch">
      {langSwitch}
   </a>
   <a href="mailto:lakelimbo@proton.me">{contact}</a>
   <a href="https://github.com/Lakelimbo">GitHub</a>
</main>
